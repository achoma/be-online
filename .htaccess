<IfModule mod_headers.c>
    Header always set Strict-Transport-Security "max-age=31536000; includeSubDomains; preload" Header set
    Content-Security-Policy "default-src 'self'; script-src 'self' https://apis.google.com; style-src 'self'
    'unsafe-inline';" Header set X-Frame-Options "DENY" Header set X-Content-Type-Options "nosniff" Header set
    Referrer-Policy "no-referrer-when-downgrade"
</IfModule>
